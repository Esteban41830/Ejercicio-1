class Email:
    __idCuenta = ''
    __dominio = ''
    __tipoDominio = ''
    __Contraseña = 1
    
    def __init__(self,idCuenta,dominio,tipoDominio,Contraseña):
      self.__idCuenta = idCuenta
      self.__dominio = dominio
      self.__tipoDominio = tipoDominio
      self.__Contraseña = Contraseña
    
    def retornaEmail(self):
      print("{}@{}{}".format(self.idCuenta,self.dominio,self.tipoDominio))
    
    def getDominio(self):
      return self.dominio
    
    def crearCuenta(self):
      self.__idCuenta = (input('Ingrese ID: '))
      self.__dominio = (input('Ingrese dominio: '))
      self.__tipoDominio = (input('Tipo dominio: '))
      self.__Contraseña = int(input('Ingrese clave: '))
    
    def cambiarClave(self):
      i = 0
      while i == 0:
         clave_actual = int(input('Ingrese clave: '))
         if self.__contraseña == clave_actual:
            self.__contraseña = int(input('Ingrese nueva clave: '))
            i = 1
         else:
            print("No hay coincidencia")

if __name__ == '__main__':
   Nombre = (input('Ingrese su nombre: '))
   cuenta1 = Email("alumnopoo","gmail",".com",12345)
   print("Estimado",Nombre,"te enviaremos los mensajes a la direccion:")
   cuenta1.retornaEmail()
   cuenta1.cambiarClave()
   cuenta2 = Email()
   cuenta2.crearCuenta()

   for i  in range (10):
      i = Email()
      i.crearCuenta()

   for i in range (10):
      c = 0
      for j in range (10):
         if i.retornaDominio == j.retornaDominio:
            c = c+1

         if (c-1) < 1:
            dominiorepetido = i.retornaDominio
            print("El dominio ", dominiorepetido,"se repite")
